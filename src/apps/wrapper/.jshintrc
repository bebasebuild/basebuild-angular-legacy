{
  "globalstrict": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "ignore",
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "white": true,
  "validthis": true,
  "eqnull" : "ignore",
  "globals": {
    "$"      : false,
    "angular": false,
    // Angular Mocks
    "inject": false,
    "module": false,
    // JASMINE
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false,
    "expect": false
  }
}
